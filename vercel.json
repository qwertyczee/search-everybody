{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/start", "dest": "/index.js" },
      { "src": "/events/(.*)", "dest": "/index.js" },
      { "src": "/results/(.*)", "dest": "/index.js" }
    ],
    "functions": {
      "index.js": {
        "maxDuration": 900
      }
    }
  }
  